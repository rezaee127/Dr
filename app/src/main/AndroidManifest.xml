<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.j23">

    <application
        android:allowBackup="true"
        android:icon="@drawable/doctor_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/doctor_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.J23">
        <activity
            android:name=".CheckPatientActivity"
            android:exported="false" />
        <activity
            android:name=".ConsultActivity"
            android:exported="false" />
        <activity
            android:name=".Activity1"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DoctorActivity"
            android:exported="true"
            android:parentActivityName=".Activity1" />
    </application>

</manifest>